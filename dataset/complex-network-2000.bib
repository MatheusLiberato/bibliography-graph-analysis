author = {Julio E Celis and Mogens Kruhøffer and Irina Gromova and Casper Frederiksen and Morten Østergaard and Thomas Thykjaer and Pavel Gromov and Jinsheng Yu and Hildur Pálsdóttir and Nils Magnusson and Torben F Ørntoft},
