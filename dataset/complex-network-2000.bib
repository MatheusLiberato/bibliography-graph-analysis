@article{Chapman20001567,
title = {Anticonvulsant activity of two metabotropic glutamate Group I antagonists selective for the mGlu5 receptor: 2-methyl-6-(phenylethynyl)-pyridine (MPEP), and (E)-6-methyl-2-styryl-pyridine (SIB 1893)},
