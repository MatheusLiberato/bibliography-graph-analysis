author = {Morten Svenson and Morten B. Hansen and Allan R. Thomsen and Marcus Diamant and Anneline Nansen and Klaus Rieneck and Ivan G. Otterness and Klaus Bendtzen},
keywords = {Cytokines (interleukin 1α)},
