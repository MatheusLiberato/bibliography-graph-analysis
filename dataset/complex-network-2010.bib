author = {P. Venkatesan and Soumen Das and M. Muthu Rama Krishnan and Chandan Chakraborty and Koel Chaudhury and Mahitosh Mandal},
keywords = {Metastasis},
keywords = {Apoptosis},
keywords = {AEE788},
