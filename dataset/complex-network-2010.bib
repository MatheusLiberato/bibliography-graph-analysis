year = {2010},
note = {\{PROCEEDINGS\} \{OF\} \{THE\} 15TH \{WORLD\} \{CONGRESS\} \{OF\} \{PSYCHOPHYSIOLOGY\} of the International Organization of Psychophysiology (I.O.P.) Budapest, Hungary September 1-4, 2010},
