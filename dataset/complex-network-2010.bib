@article{Lin20103,
title = {Linking principles of soil formation and flow regimes},
journal = {Journal of Hydrology},
volume = {393},
number = {1â€“2},
pages = {3 - 19},
year = {2010},
note = {Soil Architecture and Preferential Flow across Scales},
issn = {0022-1694},
