year = {2010},
note = {Eighth International Workshop on Information Processing in Cells and Tissues: "From Small Scale Dynamics To Understanding Systems Behavior"IPCAT 2009Eight International Workshop on Information Processing in Cells and Tissues: "From Small Scale Dynamics To Understanding Systems Behavior}, 5-9th April 2009, Ascona, Switzerland},
