author = {AristÃ³teles GÃ³es-Neto and Marcelo V.C. Diniz and Leonardo B.L. Santos and Suani T.R. Pinho and JosÃ© G.V. Miranda and Thierry Petit Lobao and Ernesto P. Borges and Charbel NiÃ±o El-Hani and Roberto F.S. Andrade},
keywords = {Chitin},
keywords = {Comparative genomics},
