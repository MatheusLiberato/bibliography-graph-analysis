pages = {351 - 359},
year = {2010},
note = {},
issn = {0003-3472},
doi = {http://dx.doi.org/10.1016/j.anbehav.2009.11.011},
url = {http://www.sciencedirect.com/science/article/pii/S0003347209005156},
author = {David M. Walker and Cristian Carmeli and F.J. PÃ©rez-BarberÃ­a and Michael Small and E. PÃ©rez-FernÃ¡ndez},
keywords = {complex system},
