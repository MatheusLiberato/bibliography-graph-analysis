@article{SchÃƒÂ¶neburg199017,
title = {Stock price prediction using neural networks: A project report},
journal = {Neurocomputing},
volume = {2},
number = {1},
pages = {17 - 27},
year = {1990},
note = {},
issn = {0925-2312},
