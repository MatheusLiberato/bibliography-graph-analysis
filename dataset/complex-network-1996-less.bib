number = {3},
pages = {383 - 388},
year = {1991},
note = {},
issn = {0029-5493},
doi = {http://dx.doi.org/10.1016/0029-5493(91)90346-J},
url = {http://www.sciencedirect.com/science/article/pii/002954939190346J},
author = {R. Rzekiecki and J.C. Malet},
abstract = {The primary thermodynamic consequences of a fire in a room are evaluated through a Ã¢â‚¬Å“multi-zonesÃ¢â‚¬ï¿½ model. \{FLAMME\} 1, the basic software package, describes and calculates for a single source the transient thermodynamic state of the various components within a closed or a ventilated room. The spread of fire to another fuel stimulated as an internal structure is only indicated by an ignition test based on the temperature of this structure. The model applies to any number of local ventilation devices. Gas venting is estimated as a flux through a hole or through a stack; furthermore, \{FLAMME\} package could be coupled to a ventilation design code involving complex network. Validation of \{FLAMME\} 1 is based on experience with pool combustion for a mixture of \{TBP\} and kerosene. Investigations are currently under way for \{PMMA\} and bituminen combustion. \{FLAMME\} 2, being developed, shows a way to extend the application range to sequential or simultaneous fire development from several sources of various materials. Some problems will arise in the modelling of radiative heat exchanges and validation will run into difficulties due to the lack of adequate automatic control experience with multiple fuel fires. "
}
@article{Thorsch1981183,
title = {Changes in the endoplasmic reticulum during differentaiation of a sieve element in Gossypium hirsutum},
journal = {Journal of Ultrastructure Research},
volume = {74},
number = {2},
pages = {183 - 194},
year = {1981},
