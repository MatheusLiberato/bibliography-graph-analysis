issn = {0022-5320},
doi = {http://dx.doi.org/10.1016/S0022-5320(81)80076-7},
url = {http://www.sciencedirect.com/science/article/pii/S0022532081800767},
author = {Jennifer Thorsch and Katherine Esau},
abstract = {In young sieve elements of Gossypium hirsutum the \{ER\} cisternae are single, bear ribosomes, and are randomly dispersed in the cytoplasm. As the sieve element nucleus begins to degencrate, \{ER\} cisternae increase in number and form stacks and convoluted aggregations. Ribosomes separate from the \{ER\} and electron-dense material appears between the contiguous cisternae. The stacked \{ER\} is most abundant at an advanced stage in nuclear degeneration and occurs mainly near the cell wall, with the cisternae cither parallel or perpendicular to the wall. Convoluted \{ER\} aggregates are found in more mature sieve elements. Occasionally the stacked and convoluted \{ER\} aggregates are continuous. Part of the stacked and convoluted forms of \{ER\} undergo further alterations. Some cisternae assume a tubular shape. Part of the \{ER\} appears as a complex networkÃ¢â‚¬â€�the parictal anastomosing ERÃ¢â‚¬â€�along the plasmalemma. This \{ER\} appears to be consistently present in mature sieve elements. "
}
@article{Tomeoka19901475,
title = {Fe-S-Ca-Al-bearing carbonaceous vein in the Yamato-74130 ureilite: Evidence for a genetic link to carbonaceous chondrites},
journal = {Geochimica et Cosmochimica Acta},
volume = {54},
number = {5},
pages = {1475 - 1481},
year = {1990},
note = {},
issn = {0016-7037},
doi = {http://dx.doi.org/10.1016/0016-7037(90)90170-P},
url = {http://www.sciencedirect.com/science/article/pii/001670379090170P},
author = {Kazushige Tomeoka and Hiroshi Takeda},
abstract = {An unusually large carbonaceous vein (C-vein) (~4.7 mm Ãƒâ€” ~0.3 mm in size) was found in the Yamato-74130 ureilite. A petrographie and scanning electron microscope study of the C-vein shows that it contains a complex network of S-rich, Fe-S-rich, Al-rich, and Si-rich regions. The Al-rich areas contain Ca-Al-rich clinopyroxenes, a Na-Ca-silica-rich phase, Fe-rich olivine, and Al-rich enstatite. Some clinopyroxenes contain up to 12.9 wt% \{A12O3\} and 5.4 wt% TiO2, resembling the fassaite that is common in Ca-Al-rich refractory inclusions in \{CV\} chondrites. The internal regions probably consist of intimate mixtures of carbonaceous material and extremely small particles rich in S, Fe-S, Al, and Si. The elements that occur in the internal regions (S, Fe, Al, Ca, and Na) are those that are strongly depleted in ureilites relative to carbonaceous chondrites. The presence of those elements in the C-vein suggests that the internal materials are remnants of drain channels for the depleted elements produced during formation process of ureilites from carbonaceous chondrites. Those elements were probably mobilized under high temperature and pressure generated by shock impacts and escaped as mixtures of liquids and vapor through fissures produced within mechanically weak C-veins. The Ti-rich fassaite may be a remnant of the CV-chondrite-like material. The C-vein provides evidence that ureilites resulted from preferential melting and recrystallization of carbonaceous chondrites by a temperature-gradient zone refining process during a planetesimal-scale collision or collisions. "
}
@article{Flesch1993316,
