@incollection{Conner19931827,
title = {New Insight into The Changing Catalyst/Polymer Morphology During Olefin Polymerization: The Application of Tomography},
editor = {L. GUCZI, F. SOLYMOSI and P. TÃƒâ€°TÃƒâ€°NYI},
booktitle = {New Frontiers in Catalysis - Proceedings of the 10th International Congress on Catalysis, Budapest, 19-24 July 1992},
publisher = {Elsevier},
