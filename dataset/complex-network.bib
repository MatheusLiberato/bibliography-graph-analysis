keywords = {Centrality},
keywords = {Modularity },
abstract = {Abstract According to asset pricing theory, a stock’s expected returns are determined by its exposure to systematic risk. In this paper, we propose a new method for analyzing the interaction effects among industries and stocks on stock returns. We construct a complex network based on correlations of abnormal stock returns and use centrality and modularity, two popular measures in social science, to determine the effect of interconnections on industry and stock returns. Supported by previous studies, our findings indicate that a relationship exists between inter-industry closeness and industry returns and between stock centrality and stock returns. The theoretical and practical contributions of these findings are discussed. }}
@article{Yang2015534,
title = {Small and flat worlds: A complex network analysis of international trade in crude oil },
