keywords = {Complex-valued neural networks},
keywords = {Global μ-stability},
keywords = {Leakage delay},
keywords = {Time-varying delays},
keywords = {Matrix inequality },
abstract = {Abstract In this paper, the global μ-stability is investigated for the complex-valued neural networks with leakage time delay and unbounded time-varying delays. The activation functions considered are no longer required to be derivable. By constructing appropriate Lyapunov–Krasovskii functionals and taking advantage of the free weighting matrix method, several sufficient and delay-dependent criteria are established to ensure the global μ-stability of the complex-valued neural networks, which can be easily checked by the Matlab Toolbox. Finally, three numerical examples are provided to illustrate the effectiveness of our theoretical results. }}
@article{Chen20157,
