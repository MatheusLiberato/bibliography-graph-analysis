abstract = {We describe a polynomial time algorithm for covering graphs with cliques, prove its asymptotic optimality in a random intersection graph model and present experimental results on complex real-world networks. }}
@article{Wei201513,
title = {Passivity analysis of impulsive coupled reaction-diffusion neural networks with and without time-varying delay },
