author = {Song Zheng and Gaogao Dong and Qinsheng Bi},
keywords = {Complex networks},
keywords = {Impulsive synchronization},
keywords = {Non-delayed and delayed coupling },
abstract = {This Letter investigates the impulsive synchronization between two complex networks with non-delayed and delayed coupling. Based on the stability analysis of impulsive differential equation, the criteria for the synchronization is derived, and a linear impulsive controller and the simple updated laws are designed. Particularly, the weight configuration matrix is not necessarily symmetric or irreducible, and the inner coupling matrix need not be symmetric. Numerical examples are presented to verify the effectiveness and correctness of the synchronization criteria. }}
@article{SolísPerales20092528,
title = {Synchronization in complex networks with distinct chaotic nodes },
journal = {Communications in Nonlinear Science and Numerical Simulation },
volume = {14},
number = {6},
