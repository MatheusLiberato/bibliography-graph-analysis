author = {Rashpal Ahluwalia and Chihui Li},
keywords = {System reliability},
keywords = {Cut-sets},
keywords = {Tie-sets},
keywords = {Complex systems },
abstract = {This paper describes an efficient approach to representation and analysis of complex networks. Typically, an n node system is represented by an n × n connection matrix. This paper presents a new connection matrix representation scheme that uses three fields; “begin node”, “end node”, and “component id” to represent each node in the network. The proposed approach to connection matrix representation is more concise than the n × n matrix, which is often sparsely populated. This paper also describes network simplification algorithm based on the revised connection matrix. The algorithm when applied to a large system with 55 tie-sets reduced the network to a single tie-set. }}
@article{Thilaga201532,
title = {A heuristic branch-and-bound based thresholding algorithm for unveiling cognitive activity from \{EEG\} data },
journal = {Neurocomputing },
volume = {170},
number = {},
pages = {32 - 46},
