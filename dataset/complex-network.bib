abstract = {In this paper, we proposed an efficient immunization—“high-risk immunization”. The standard \{SIRS\} model was modified, respectively, on \{WS\} small-world network and \{BA\} scale-free network. Based on our new \{SIRS\} model, the density of infected individuals was analyzed from a theoretical point of view, and computer simulation was implemented on different networks. The results indicate that the high-risk immunization is effective, and it is economic and feasible in practice. }}
@article{Pu20124420,
title = {Robustness analysis of network controllability },
