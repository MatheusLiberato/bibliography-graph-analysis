keywords = {Complex networks},
keywords = {Community structure},
keywords = {Core-vertex},
keywords = {Intimate degree },
abstract = {In this paper, we present a new approach to extract communities in the complex networks with considerable accuracy. We introduce the core-vertex and the intimate degree between the community and its neighboring vertices. First, we find the core-vertices as the initial community. These core-vertices are then expanded using intimate degree function during extracting community structure from the given network. In addition, our algorithm successfully finds common nodes between communities. Experimental results using some real-world networks data shows that the performance of our algorithm is satisfactory. }}
@article{Lv2015533,
title = {Soft computing for overflow particle size in grinding process based on hybrid case based reasoning },
journal = {Applied Soft Computing },
volume = {27},
number = {},
pages = {533 - 542},
year = {2015},
note = {},
issn = {1568-4946},
doi = {http://dx.doi.org/10.1016/j.asoc.2014.09.035},
url = {http://www.sciencedirect.com/science/article/pii/S1568494614004888},
author = {Zheng Lv and Ying Liu and Jun Zhao and Wei Wang},
keywords = {Overflow particle size},
keywords = {Soft computing},
keywords = {Case based reasoning},
keywords = {Community finding },
abstract = {Abstract The overflow particle size of cyclone is one of the most significant performance indices in the process of ore grinding. Given the measuring difficulty under the current industrial conditions, a hybrid method, which combines community finding (CF) of a complex network with case-based reasoning (CBR) is proposed in this study. The \{CF\} method with a new evaluation criterion for the vertex combination degree is designed to select the typical cases from the constructed communities, and a k-nearest neighbors (k-NN) based strategy with multi-similarity threshold is proposed in the case retrieval process. To verify the effectiveness of the proposed method, a number of comparative simulations by using the real-world data coming from a copper–molybdenum concentration plant are carried out, and the results indicate that the proposed method can provide a good measure quality for the industrial application. }}
@article{Qier2014369,
title = {Embodied Energy Flow Network of Chinese Industries: A Complex Network Theory Based Analysis },
journal = {Energy Procedia },
volume = {61},
number = {},
pages = {369 - 372},
year = {2014},
