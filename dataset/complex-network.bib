author = {Ping Li and Zhang Yi and Lei Zhang},
abstract = {Global synchronization of a class of complex networks with time-varying delays is investigated in this paper. Some sufficient conditions are derived. These conditions show that the synchronization of delayed complex networks can be determined by their topologies. In addition, these conditions are simply represented in terms of the networks coupling matrix and are easy to be checked. A typical example of complex networks with chaotic nodes is employed to illustrate the obtained global synchronization results. }}
@article{Du201457,
title = {A new method of identifying influential nodes in complex networks based on \{TOPSIS\} },
