abstract = {This paper investigates the global synchronization of nonlinearly coupled complex networks with non-delayed and delayed coupling using methods that are based on pinning control. Sufficient conditions for global synchronization are obtained by applying suitable feedback or adaptive feedback controllers to certain selected nodes and numerical examples are provided to demonstrate the effectiveness of the theory. }}
@article{Wang20085616,
title = {Synchronization in a class of weighted complex networks with coupling delays },
journal = {Physica A: Statistical Mechanics and its Applications },
