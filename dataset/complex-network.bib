doi = {http://dx.doi.org/10.1016/j.jfranklin.2013.06.016},
url = {http://www.sciencedirect.com/science/article/pii/S001600321300241X},
author = {Shidong Zhai and Xiao-Song Yang},
abstract = {Abstract This paper is concerned with the synchronization of complex switched networks with different inner coupling matrices. By using contraction theory, a synchronization criterion is established by the contractivity of lower-dimensional dynamic systems under some conditions on coupling matrices. Two examples are provided to illustrate the effectiveness of the criterion. }}
@article{Xu20082789,
title = {A novel definition of generalized synchronization on networks and a numerical simulation example },
journal = {Computers & Mathematics with Applications },
volume = {56},
number = {11},
pages = {2789 - 2794},
