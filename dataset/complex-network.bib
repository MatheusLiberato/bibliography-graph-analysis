@article{WachsLopes20168,
title = {Analyzing natural human language from the point of view of dynamic of a complex network },
journal = {Expert Systems with Applications },
volume = {45},
number = {},
pages = {8 - 22},
year = {2016},
note = {},
issn = {0957-4174},
doi = {http://dx.doi.org/10.1016/j.eswa.2015.09.020},
