journal = {Chemometrics and Intelligent Laboratory Systems },
volume = {118},
number = {},
pages = {317 - 323},
year = {2012},
note = {},
issn = {0169-7439},
doi = {http://dx.doi.org/10.1016/j.chemolab.2012.05.001},
