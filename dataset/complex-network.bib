volume = {86},
number = {},
pages = {28 - 34},
year = {2016},
note = {},
issn = {0960-0779},
doi = {http://dx.doi.org/10.1016/j.chaos.2016.01.027},
url = {http://www.sciencedirect.com/science/article/pii/S0960077916300182},
author = {Chen Hong and Jun Zhang and Xian-Bin Cao and Wen-Bo Du},
keywords = {Multilayer network},
keywords = {Chinese air transportation network},
keywords = {Structural property},
keywords = {Complex network },
abstract = {Abstract Recently multilayer networks are attracting great attention because the properties of many real-world systems cannot be well understood without considering their different layers. In this paper, we investigate the structural properties of the Chinese air transportation multilayer network (ATMN) by progressively merging layers together, where each commercial airline (company) defines a layer. The results show that the high clustering coefficient, short characteristic path length and large collection of reachable destinations of the Chinese \{ATMN\} can only emerge when several layers are merged together. Moreover, we compare two main types of layers corresponding to major and low-cost airlines. It is found that the small-world property and the rich-club effect of the Chinese \{ATMN\} are mainly caused by those layers corresponding to major airlines. Our work will highlight a better understanding of the Chinese air transportation network. }}
@article{Chen20092741,
title = {A fast and efficient heuristic algorithm for detecting community structures in complex networks },
journal = {Physica A: Statistical Mechanics and its Applications },
volume = {388},
number = {13},
pages = {2741 - 2749},
year = {2009},
note = {},
issn = {0378-4371},
doi = {http://dx.doi.org/10.1016/j.physa.2009.03.022},
url = {http://www.sciencedirect.com/science/article/pii/S0378437109002209},
author = {Duanbing Chen and Yan Fu and Mingsheng Shang},
keywords = {Complex networks},
keywords = {Heuristic algorithm},
keywords = {Community structure},
keywords = {Community size},
keywords = {Connecting degree },
abstract = {Community structure is an important property of complex networks. How to detect the communities is significant for understanding the network structure and to analyze the network properties. Many algorithms, such as K–L and GN, have been proposed to detect community structures in complex networks. According to daily experience, a community should have many nodes and connections. Based on these principles and existing researches, a fast and efficient algorithm for detecting community structures in complex networks is proposed in this paper. The key strategy of the algorithm is to mine a node with the closest relations with the community and assign it to this community. Four real-world networks are used to test the performance of the algorithm. Experimental results demonstrate that the algorithm proposed is rather efficient for detecting community structures in complex networks. }}
@article{Yang2006544,
title = {Collective chaos induced by structures of complex networks },
