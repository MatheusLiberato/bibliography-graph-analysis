journal = {Basal Ganglia },
volume = {5},
number = {1},
pages = {11 - 14},
year = {2015},
note = {},
issn = {2210-5336},
doi = {http://dx.doi.org/10.1016/j.baga.2015.02.001},
url = {http://www.sciencedirect.com/science/article/pii/S2210533615000039},
author = {Saak V. Ovsepian and Károly Elekes},
keywords = {Forebrain systems},
keywords = {Neuronal circuits},
keywords = {Network architecture},
keywords = {Neuro-informatics},
keywords = {Basalo-cortical},
keywords = {Prefrontal cortex },
abstract = {Abstract A conference was organized by the Hungarian Academy of Sciences in honour of Professor Laszlo Zaborszky, to recapitulate the progress and identify principal directions for future studies of circuit architecture and connections in forebrain systems. Neuroscientists gathered in Tihany to present and debate latest discoveries of the organizational rules and functions of local and long-range basal forebrain projections with their appraisal in the context of network theory. Here, we report on the main themes in basic and translational research that were discussed and reflect on conceptual and technical developments in untangling the inner design and mechanisms of deep forebrain circuits. }}
@article{Ding20136603,
title = {Studies on controllability of directed networks with extremal optimization },
journal = {Physica A: Statistical Mechanics and its Applications },
