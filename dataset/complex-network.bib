author = {Ju-ping Zhang and Zhen Jin},
keywords = {Complex networks},
keywords = {Community structure},
keywords = {Reproduction number},
keywords = {Immunization},
keywords = {Final sizes },
abstract = {In this paper, we study an \{SEAIR\} epidemic network model with community structure. Different spreading rates in our model are adopted to describe the epidemic behavior of nodes in the inner and outer community, which makes the model more practical. We obtain the basic reproduction number. The effects of two immunization schemes are studied and compared. Finally, a final size relation is derived for network epidemic models with community structure. The derivation depends on an explicit formula. }}
@article{Ahluwalia2011525,
title = {An efficient approach to representation and simplification of complex networks },
