issn = {0925-2312},
doi = {http://dx.doi.org/10.1016/j.neucom.2014.04.087},
url = {http://www.sciencedirect.com/science/article/pii/S0925231214013551},
author = {Yuxin Zhao and Wen Jiang and Shenghong Li and Yinghua Ma and Guiyang Su and Xiang Lin},
keywords = {Community detection},
keywords = {Complex network},
keywords = {Modularity optimization},
keywords = {Resolution limit},
keywords = {Cellular learning automata },
abstract = {Abstract Community structure is a common and important property of complex networks. The detection of communities has great significance for understanding the function and organization of networks. Generally, community detection can be formulated as a modularity optimization problem. However, traditional modularity optimization based algorithms have the resolution limit that they may fail to find communities which are smaller than a certain size. In this paper, we propose a cellular learning automata based algorithm for detecting communities in complex networks. Our algorithm models the whole network as an irregular cellular learning automata (ICLA) and reveals the optimal community structure through the evolution of the cellular learning automata. By interacting with both the global and local environments, our algorithm effectively solves the resolution limit problem of modularity optimization. The experiments on both synthetic and real-world networks demonstrate that our algorithm is effective and efficient at detecting community structure in complex networks. }}
@article{An20141067,
