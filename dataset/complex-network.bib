note = {Nicotinic Acetylcholine Receptors as Therapeutic Targets: Emerging Frontiers in Basic Research and Clinical Science (Satellite to the 2013 Meeting of the Society for Neuroscience) },
issn = {0006-2952},
doi = {http://dx.doi.org/10.1016/j.bcp.2013.06.011},
