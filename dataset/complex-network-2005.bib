url = {http://www.sciencedirect.com/science/article/pii/S1567423109700279},
author = {Anil N. Rama and S. Charles Cho and Clete A. Kushida},
abstract = {Publisher Summary The purpose of this chapter is to provide an overview of the salient features of non-rapid eye movement (NREM) sleep and rapid eye movement (REM) sleep. Sleep is comprised of two distinct states known as \{NREM\} and \{REM\} sleep. \{NREM\} sleep is subdivided into four stages: stage 1, stage 2, stage 3, and stage 4. Stages 3 and 4 are collectively referred to as slow-wave sleep. \{REM\} sleep may be subdivided into two stages: phasic and tonic. \{NREM\} and \{REM\} sleep are associated with dynamic changes in the autonomic nervous system and immune system. The NREM–REM sleep cycle occurs every 90 minutes and approximately 4–6 cycles occur per major sleep episode. The ratio of \{NREM\} sleep to \{REM\} sleep in each cycle varies during the course of the night. The early cycles are dominated by slow-wave sleep and the later cycles are dominated by \{REM\} sleep. The first episode of \{REM\} sleep may last only a few minutes and subsequent \{REM\} episodes progressively lengthen in duration during the course of the major sleep period.}}
@incollection{VertosickJr2005431,
title = {23 A network approach to living tissues},
editor = {Ray Paton and Laura A. McNamara},
booktitle = {Multidisciplinary Approaches to Theory in Medicine},
publisher = {Elsevier},
year = {2005},
volume = {3},
pages = {431 - 445},
series = {Studies in Multidisciplinarity},
issn = {1571-0831},
doi = {http://dx.doi.org/10.1016/S1571-0831(06)80027-4},
url = {http://www.sciencedirect.com/science/article/pii/S1571083106800274},
author = {Frank T. Vertosick Jr.},
abstract = {Publisher Summary Networks are ensembles of nodes connected into a single computational device. Originally intended to model nerve cells and electrical amplifiers connected by fixed axons or wires, networks are used to model any ensemble of interacting elements. They are fast, robust, handle patterned data well, and require no external programming. They can be built of simple repeated modules using a limited set of rules. All these features make them ideal for application in living systems. Networks are the preferred architecture for all intelligent biosystems, from the cytoplasm to large ecosystems. The brain, which is the original inspiration for network theory, is just the latest version of a network paradigm that originated with the advent of life itself. Network theory allows a deeper insight into the intellect of biological machines, including the cancerous cell, the diseased brain, and the vertebrate immune system. The scale-free topology of living networks may afford an immediate insight into the way to target living networks, although its utility is limited by the inherent adaptability of living networks.}}
