number = {7},
pages = {647 - 658},
year = {2005},
note = {Biological responses to \{DNA\} damage},
issn = {0300-9084},
doi = {http://dx.doi.org/10.1016/j.biochi.2005.05.003},
url = {http://www.sciencedirect.com/science/article/pii/S0300908405001239},
