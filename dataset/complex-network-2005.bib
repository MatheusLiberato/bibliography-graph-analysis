journal = {Ecological Modelling},
volume = {186},
number = {4},
pages = {489 - 501},
year = {2005},
note = {Emergent Properties in Individual-based Models Case Studies from the Bornhöved Project (Northern Germany)},
issn = {0304-3800},
doi = {http://dx.doi.org/10.1016/j.ecolmodel.2005.02.014},
url = {http://www.sciencedirect.com/science/article/pii/S0304380005001389},
author = {Hauke Reuter and Franz Hölker and Ulrike Middelhoff and Fred Jopp and Christiane Eschenbach and Broder Breckling},
keywords = {Emergent properties},
keywords = {Collective properties},
keywords = {Self-organisation},
keywords = {Ecological complexity},
keywords = {Individual-based model},
