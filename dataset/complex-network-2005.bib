author = {Andreas Schadschneider and Wolfgang Knospe and Ludger Santen and Michael Schreckenberg},
keywords = {Cellular automata},
keywords = {Modelling of traffic flow},
keywords = {Optimization},
abstract = {The understanding of traffic dynamics on highway networks is essential for traffic control. Nowadays realistic microscopic models exist that allow highly efficient (faster-than-real-time) simulations of large networks. These simulations can be used not only to determine the state of the network from incomplete information, but also for traffic forecasting. We also discuss an analysis of the highway network in Germany to identify bottlenecks and possible optimization strategies.}}
@article{Bose200549,
title = {Motifs in gene transcription regulatory networks},
journal = {Physica A: Statistical Mechanics and its Applications},
volume = {346},
number = {1–2},
pages = {49 - 57},
year = {2005},
note = {Statphys - Kolkata V: Proceedings of the International Conference on Statistical Physics: "Complex Networks: Structure, Function and Processes"},
issn = {0378-4371},
