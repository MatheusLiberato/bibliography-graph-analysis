@incollection{Garavelli2005191,
title = {VI. Computation of photochemical reaction mechanisms in organic chemistry},
editor = {M. Olivucci},
booktitle = {Computational Photochemistry},
publisher = {Elsevier},
year = {2005},
volume = {16},
pages = {191 - 223},
