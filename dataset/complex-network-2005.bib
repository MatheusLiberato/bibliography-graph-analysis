keywords = {Rickettsia},
keywords = {Proteomics},
keywords = {GroEL},
keywords = {ORFan},
abstract = {The availability of genome sequence offers the opportunity to further expand our knowledge about proteins expressed by Rickettsia conorii, strictly intracellular bacterium responsible for Mediterranean spotted fever. Using two-dimensional polyacrylamide gel electrophoresis combined with MALDI-TOF mass spectrometry, we established the first reference map of R. conorii proteome. This approach also allowed identification of GroEL as the major antigen recognized by rabbit serum and sera of infected patients. Altogether, this work opens the way to characterize the proteome of R. conorii, to compare protein profiles of different isolates or of bacteria maintained under different experimental conditions and to identify immunogenic proteins as potential vaccine targets.}}
@article{Singh2005392,
