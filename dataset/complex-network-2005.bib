issn = {1053-8119},
doi = {http://dx.doi.org/10.1016/j.neuroimage.2005.01.053},
url = {http://www.sciencedirect.com/science/article/pii/S1053811905000868},
author = {M.M. Strauss and N. Makris and I. Aharon and M.G. Vangel and J. Goodman and D.N. Kennedy and G.P. Gasic and H.C. Breiter},
keywords = {Aversion},
