abstract = {A deliberation upon the possibility of generating a comprehensive view of ‘mind as a whole' by integrating biology, psychology and sociology, and considering ‘Mind' as a dynamical interplay between values existing over many levels and scales of complex systems. This view of mind as a coevolutionary whole is related to similar ecological viewpoints being developed in the fields of artificial life and multi-agent systems. A meta-model of mind is proposed which attempts to integrate the various existing views of mind from a time perspective, and positions the field in a way that we think augurs well for the potential of mind studies based upon modern complex systems science concepts.}}
@incollection{Tishby2005787,
title = {Course 16 The emergence of relevant data representations: An information theoretic approach},
editor = {C.C. Chow, B. Gutkin, D. Hansel, C. Meunier and J. Dalibard},
