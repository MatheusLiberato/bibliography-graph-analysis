author = {Hideaki Kaneto and Yoshihisa Nakatani and Munehide Matsuhisa and Yoshimitsu Yamasaki},
abstract = {Insulin resistance as well as pancreatic β-cell dysfunction is observed in type 2 diabetes. Recently, it has been shown that increase of endoplasmic reticulum (ER) stress and activation of the c-Jun N-terminal kinase (JNK) pathway are involved in the progression of insulin resistance found in diabetes. Indeed, suppression of \{ER\} stress or the \{JNK\} pathway in diabetic mice improves insulin resistance and ameliorates glucose tolerance. Thus, increase of \{ER\} stress and activation of the \{JNK\} pathway play a central role in pathogenesis of type 2 diabetes and could be a potential target for diabetes therapy. Section editors: James Fraser – Pfizer, \{USA\} Anne Reifel Miller – Lilly Research Laboratories, \{USA\}}}
@article{Darzacq2005332,
