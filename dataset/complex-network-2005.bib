issn = {0165-0173},
doi = {http://dx.doi.org/10.1016/j.brainresrev.2004.10.001},
url = {http://www.sciencedirect.com/science/article/pii/S0165017304001432},
author = {Silvia H. Madalosso and Eva M. Pérez-Villegas and José A. Armengol},
keywords = {Basilar pontine nuclei},
