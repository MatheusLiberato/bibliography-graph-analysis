pages = {20 - 26},
year = {2005},
note = {Statphys - Kolkata V: Proceedings of the International Conference on Statistical Physics: "Complex Networks: Structure, Function and Processes"},
issn = {0378-4371},
doi = {http://dx.doi.org/10.1016/j.physa.2004.08.045},
url = {http://www.sciencedirect.com/science/article/pii/S0378437104011574},
author = {T. Aste and T. Di Matteo and S.T. Hyde},
keywords = {Networks},
keywords = {Complex systems},
keywords = {Hyperbolic graphs},
keywords = {Econophysics},
abstract = {We explore a novel method to generate and characterize complex networks by means of their embedding on hyperbolic surfaces. Evolution through local elementary moves allows the exploration of the ensemble of networks which share common embeddings and consequently share similar hierarchical properties. This method provides a new perspective to classify network-complexity both on local and global scale. We demonstrate by means of several examples that there is a strong relation between the network properties and the embedding surface.}}
