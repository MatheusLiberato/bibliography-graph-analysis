author = {Kamalika Basu Hajra and Parongama Sen},
keywords = {Preferential attachment},
keywords = {Time dependence},
keywords = {Real world networks},
abstract = {In many growing networks, the age of the nodes plays an important role in deciding the attachment probability of the incoming nodes. For example, in a citation network, very old papers are seldom cited while recent papers are usually cited with high frequency. We study actual citation networks to find out the distribution T ( t ) of t, the time interval between the published and the cited paper. For different sets of data we find a universal behaviour: T ( t ) ∼ t - 0.9 for t ⩽ t c and T ( t ) ∼ t - 2 for t &gt; t c where t c ∼ O ( 10 ) .}}
@article{Blanchi200523,
title = {Mutations of brainstem transcription factors and central respiratory disorders},
journal = {Trends in Molecular Medicine},
