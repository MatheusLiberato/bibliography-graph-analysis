abstract = {A recently introduced measure of bipartivity in complex networks is tested in the role of a selector of stable fullerene isomers. Its performance is compared with other proposed stability predictors on the set of experimentally characterized fullerene isomers. It is found out that the new predictor exhibits a high degree of selectivity. This, together with the simplicity of its implementation and low computational complexity, makes it a useful tool in narrowing the set of candidates for stable fullerene isomers.}}
@article{Tadić2005183,
title = {Search and topology aspects in transport on scale-free networks},
journal = {Physica A: Statistical Mechanics and its Applications},
volume = {346},
number = {1–2},
pages = {183 - 190},
year = {2005},
note = {Statphys - Kolkata V: Proceedings of the International Conference on Statistical Physics: "Complex Networks: Structure, Function and Processes"},
issn = {0378-4371},
doi = {http://dx.doi.org/10.1016/j.physa.2004.08.065},
url = {http://www.sciencedirect.com/science/article/pii/S037843710401177X},
author = {Bosiljka Tadić and Stefan Thurner},
keywords = {Scale-free networks},
keywords = {Information traffic},
keywords = {Waiting times},
abstract = {Using a recently proposed model Physica A 332 (2004) 566 of information transport on complex networks we study the role of network substrates on the statistics of queuing times and correlations in traffic streams. When navigation with an enlarged information horizon is applied the waiting time distribution on structured networks has a power-law tail which can be parametrized by 1 / ( 1 - q ) , where the respective value of the non-extensivity parameter q increases systematically with increasing graph complexity. The corresponding distribution on the small-world graph appears to be a stretched-exponential function that cannot be classified within the same scheme. We further demonstrate by looking at the anti-persistence in flow time-series and the transit time distributions, how traffic efficiency increases with an extended information horizon. A horizon of two layers approaches the critical horizon on the correlated cyclic scale-free graph, suggesting that the mechanisms of signaling over two-to-three layers ensure efficient transport processes on networks with this organizational complexity.}}
@article{Andrade20051,
title = {Spectral properties of the Apollonian network},
