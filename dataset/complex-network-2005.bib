url = {http://www.sciencedirect.com/science/article/pii/S0006322305005007},
author = {Edythe D. London and Steven M. Berman and Bradley Voytek and Sara L. Simon and Mark A. Mandelkern and John Monterosso and Paul M. Thompson and Arthur L. Brody and Jennifer A. Geaga and Michael S. Hong and Kiralee M. Hayashi and Richard A. Rawson and Walter Ling},
keywords = {Methamphetamine},
