year = {2005},
note = {},
issn = {0021-9517},
doi = {http://dx.doi.org/10.1016/j.jcat.2004.11.029},
url = {http://www.sciencedirect.com/science/article/pii/S0021951704005652},
author = {Marcus Crocoll and Sven Kureti and Werner Weisweiler},
keywords = {Kinetics},
