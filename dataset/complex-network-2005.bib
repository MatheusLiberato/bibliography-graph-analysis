author = {Sigrid Karrer and Anja Kathrin Bosserhoff and Petra Weiderer and Oliver Distler and Michael Landthaler and Rolf-Markus Szeimies and Ulf Müller-Ladner and Jürgen Schölmerich and Claus Hellerbrand},
keywords = {chemokine},
