url = {http://www.sciencedirect.com/science/article/pii/S0378437113004950},
author = {Martin Knor and Riste Škrekovski},
keywords = {Complex systems},
keywords = {Small world network},
keywords = {Scale-free network},
keywords = {Deterministic model},
abstract = {Abstract Using very symmetric graphs we generalize several deterministic self-similar models of complex networks and we calculate the main network parameters of our generalization. More specifically, we calculate the order, size and the degree distribution, and we give an upper bound for the diameter and a lower bound for the clustering coefficient. These results yield conditions under which the network is a self-similar and scale-free small world network. We remark that all these conditions are posed on a small base graph which is used in the construction. As a consequence, we can construct complex networks having prescribed properties. We demonstrate this fact on the clustering coefficient. We propose eight new infinite classes of complex networks. One of these new classes is so rich that it is parametrized by three independent parameters.}}
@article{Liu2014506,
title = {Failure Propagation Analysis of Aircraft Engine Systems Based on Complex Network},
