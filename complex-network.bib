abstract = {Abstract In this work we investigate the problem of controlling processes hold by an ensemble of autonomous mobile agents in a plane. As the autonomous mobile agents move in the plane they can interact to each other depending on the distance among them, defining a network of time-varying topological structure. Particularly, we investigate the problem of synchronizing processes hold by each mobile agent of a time-varying network by applying the fully decentralized adaptive pinning control strategy to guarantee pinning synchronization of the mobile agent network under fast-switching constraint. We have also proposed a new motion model to define agents motion in the plane. Numerical simulations are given to validate the proposal.}}
@article{Donovan2016211,
title = {T cell and reticular network co-dependence in HIV infection},
journal = {Journal of Theoretical Biology},
volume = {395},
