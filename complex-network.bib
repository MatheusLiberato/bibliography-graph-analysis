author = {Yong Liu and Claudia Giovanna Peñuelas-Rivas and Esvieta Tenorio-Borroto and María Rivas-Guevara and Germán Buendía-Rodríguez and Zhiliang Tan and Humberto González-Díaz},
keywords = {Complex networks},
keywords = {Artificial neural networks},
