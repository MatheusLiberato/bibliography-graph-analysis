url = {http://www.sciencedirect.com/science/article/pii/S2451902215000051},
author = {Cristian E. Leyton and Ben Cassidy and Victor L. Villemagne and Gareth Jones and John B. Kwok and Christopher C. Rowe and Kirrie J. Ballard and Olivier Piguet and John R. Hodges},
keywords = {Alzheimer's disease},
