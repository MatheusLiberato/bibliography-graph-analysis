url = {http://www.sciencedirect.com/science/article/pii/S0195669815001080},
author = {Dinh Van Le},
abstract = {Abstract Let ( h 0 , h 1 , … , h s ) with h s ≠ 0 be the h -vector of the broken circuit complex of a series–parallel network M . Let G be a graph whose cycle matroid is M . We give a formula for the difference h s − 1 − h 1 in terms of an ear decomposition of G . A number of applications of this formula are provided, including several bounds for h s − 1 − h 1 , a characterization of outerplanar graphs, and a solution to a conjecture on A -graphs posed by Fenton. We also prove that h s − 2 ≥ h 2 when s ≥ 4 .}}
@article{Schieber2016359,
