url = {http://www.sciencedirect.com/science/article/pii/S0378437115008572},
author = {Song Liang and Ranchao Wu and Liping Chen},
keywords = {Complex networks with delay},
keywords = {Fractional-order},
keywords = {Adaptive control},
keywords = {Pinning control},
abstract = {Abstract Based on the stability theory of fractional-order systems, synchronization of general fractional-order uncertain complex networks with delay is investigated in this paper. By the inequality of the fractional derivative and the comparison principle of the linear fractional equation with delay, synchronization of complex networks with delay is realized under adaptive control. Some sufficient criteria ensuring local asymptotical synchronization under adaptive control and global asymptotical synchronization under adaptive pinning control are derived, respectively. Finally, numerical simulations are presented to demonstrate the validity and feasibility of the proposed synchronization criteria.}}
@article{Wang201689,
title = {Pinning synchronization of nonlinearly coupled complex networks with time-varying delays using M-matrix strategies},
