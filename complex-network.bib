author = {Yue Wang and Michael Halper and Duo Wei and Huanying Gu and Yehoshua Perl and Junchuan Xu and Gai Elhanan and Yan Chen and Kent A. Spackman and James T. Case and George Hripcsak},
keywords = {SNOMED},
