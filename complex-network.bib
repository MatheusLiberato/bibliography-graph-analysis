url = {http://www.sciencedirect.com/science/article/pii/S0005109816300899},
author = {Shiyu Zhao and Daniel Zelazo},
keywords = {Sensor network},
keywords = {Network localizability},
keywords = {Distributed localization},
keywords = {Bearing rigidity},
keywords = {Bearing Laplacian},
abstract = {Abstract This paper addresses the problem of bearing-based network localization, which aims to localize all the nodes in a static network given the locations of a subset of nodes termed anchors and inter-node bearings measured in a common reference frame. The contributions of the paper are twofold. Firstly, we propose necessary and sufficient conditions for network localizability with both algebraic and rigidity theoretic interpretations. Secondly, we propose and analyze a linear distributed protocol for bearing-based network localization. One novelty of our work is that the localizability analysis and localization protocol are applicable to networks in arbitrary dimensional spaces.}}
@article{Sirk2016178,
title = {Bi-modal polymer networks: Viscoelasticity and mechanics from molecular dynamics simulation},
journal = {Polymer},
