keywords = {Semi-Markovian switching},
keywords = {Observer-based control},
keywords = {Sliding mode control},
abstract = {Abstract This paper focuses on the problem of sliding mode control for networked control systems with semi-Markovian switching and random measurement, where the measurement channel is assumed to suffer from random sensor delays. A Luenberger observer is designed to generate the estimation of system states, and the integral sliding surface is constructed to guarantee exponential stability of networked semi-Markovian switching systems in the mean square sense. Then a proper controller is synthesized to ensure that the trajectory of the closed-loop networked semi-Markovian switching systems can be driven onto the prescribed sliding surface. Finally, numerical examples and simulations are provided to illustrate the effectiveness of the integral sliding mode control scheme.}}
@article{Wang20101699,
title = {Robustness of pinning a general complex dynamical network},
journal = {Physics Letters A},
volume = {374},
