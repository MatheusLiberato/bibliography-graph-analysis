number = {},
pages = {96 - 103},
year = {2014},
note = {},
issn = {0096-3003},
doi = {http://dx.doi.org/10.1016/j.amc.2013.12.083},
url = {http://www.sciencedirect.com/science/article/pii/S0096300313013738},
