url = {http://www.sciencedirect.com/science/article/pii/S0303264716300090},
author = {Joseph Xu Zhou and Areejit Samal and Aymeric Fouquier d'Hérouël and Nathan D. Price and Sui Huang},
keywords = {Gene regulatory network (GRN)},
