url = {http://www.sciencedirect.com/science/article/pii/S0926860X13003967},
author = {Misbahu Ladan Mohammed and Dipesh Patel and Rene Mbeleck and Debdarsan Niyogi and David C. Sherrington and Basudeb Saha},
keywords = {Alkene epoxidation},
