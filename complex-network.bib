issn = {2405-8963},
doi = {http://dx.doi.org/10.1016/j.ifacol.2015.06.251},
url = {http://www.sciencedirect.com/science/article/pii/S2405896315004905},
author = {Cristina Ruiz-Martin and Adolfo López Paredes and Gabriel A. Wainer},
