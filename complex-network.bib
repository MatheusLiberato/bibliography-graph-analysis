abstract = {Abstract As traffic congestion becoming increasingly prominent, more and more cities begin to focus on the development of subway. It is of great significance to study the subway network characteristics and evolution properties based on the complex network theory. In this paper, we take Guangzhou subway network (GSN) as a case and present its six characteristics. Furthermore, the properties of the future GSN are systematically analyzed and its evolution properties are also investigated. The conclusion may be useful for the development of GSN in the future.}}
@article{Peker2016,
title = {A new approach for automatic sleep scoring: Combining Taguchi based complex-valued neural network and complex wavelet transform},
journal = {Computer Methods and Programs in Biomedicine},
