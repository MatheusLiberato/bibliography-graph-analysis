author = {Jiaxing Shang and Lianchen Liu and Xin Li and Feng Xie and Cheng Wu},
keywords = {Complex networks},
keywords = {Epidemic spreading},
keywords = {Community structure},
keywords = {Overlapping},
abstract = {Abstract Many real-world networks exhibit community structure where vertices belong to one or more communities. Recent studies show that community structure plays an import role in epidemic spreading. In this paper, we investigate how the extent of overlap among communities affects epidemics. In order to experiment on the characteristic of overlapping communities, we propose a rewiring algorithm that can change the community structure from overlapping to non-overlapping while maintaining the degree distribution of the network. We simulate the Susceptible-Infected-Susceptible (SIS) epidemic process on synthetic scale-free networks and real-world networks by applying our rewiring algorithm. Experiments show that epidemics spread faster on networks with higher level of overlapping communities. Furthermore, overlapping communities' effect interacts with the average degree's effect. Our work further illustrates the important role of overlapping communities in the process of epidemic spreading.}}
@article{Li2015645,
title = {Dissipativity analysis of memristor-based complex-valued neural networks with time-varying delays},
journal = {Information Sciences},
volume = {294},
number = {},
pages = {645 - 665},
year = {2015},
note = {Innovative Applications of Artificial Neural Networks in Engineering},
issn = {0020-0255},
doi = {http://dx.doi.org/10.1016/j.ins.2014.07.042},
url = {http://www.sciencedirect.com/science/article/pii/S0020025514007518},
