abstract = {Abstract This work presents an effective solution to reduce the computational complexity order of behavioral model generation for reflective semiconductor optical amplifier (RSOA). The proposed model is based on a complex valued (CV) neural network (CVNN) structure, using polar CV basis functions architecture. The CV model parameters are extracted by means of nonlinear complex-domain Levenberg–Marquardt algorithm, from recorded experimental 20 Msymbol/s 64-quadrature amplitude modulation (QAM) input–output data. The evaluation results of polar CVNN model prove to be more adequate to accurately describe the nonlinear dynamic magnitude and phase distortions of RSOA, compared to double-input double-output real-valued neural network (RVNN) rectangular structure. Additionally, significant reduction of the computational cost is achieved in comparison to the RVNN approach.}}
@article{Wang2013219,
title = {Robustness of complex networks with the local protection strategy against cascading failures},
journal = {Safety Science},
volume = {53},
number = {},
pages = {219 - 225},
year = {2013},
note = {},
issn = {0925-7535},
doi = {http://dx.doi.org/10.1016/j.ssci.2012.09.011},
url = {http://www.sciencedirect.com/science/article/pii/S0925753512002317},
author = {Jianwei Wang},
keywords = {Cascading failure},
keywords = {Complex network},
keywords = {BA network},
keywords = {Mitigation strategy},
keywords = {Power grid},
abstract = {Considering the role of the neighboring nodes of an overload node, we articulate a local protection strategy to address the problem of the optimal defense in the cascading propagation. From two aspects of the global robustness and the different attacks, we numerically demonstrate the effectiveness of this strategy on Barabási–Albert (BA) scale-free networks and the power grid, and show that the robustness of diverse networks against cascading failures can be improved dramatically. And we numerically find the optimal value of the parameter, at which two types of networks can reach the strongest robust level against cascading failures. Next, in BA networks we verify this finding by theoretical analysis. Our results may be very useful for constructing the optimal protection strategy in realistic networks and for leading to insights into the mitigation of cascading failures.}}
@article{Jin2013316,
title = {Adaptive pinning synchronization of a class of nonlinearly coupled complex networks},
journal = {Communications in Nonlinear Science and Numerical Simulation},
volume = {18},
number = {2},
pages = {316 - 326},
year = {2013},
note = {},
issn = {1007-5704},
doi = {http://dx.doi.org/10.1016/j.cnsns.2012.07.011},
url = {http://www.sciencedirect.com/science/article/pii/S1007570412003012},
author = {Xiao-Zheng Jin and Guang-Hong Yang},
keywords = {Dynamical complex networks},
keywords = {Synchronization},
keywords = {Adaptive pinning control},
keywords = {Nonlinear couplings},
abstract = {This paper is concerned with the pinning control of the robust synchronization of a class of nonlinearly coupled complex networks through adaptive techniques. The effect of perturbed couplings is addressed by adaptive compensation and adjustment methods with controllers and coupling strength designs, respectively. For the pinned nodes, a controller gain function is proposed to compensate the nonlinearities based on adaptive estimations of controller parameters on-line; while for the un-pinned nodes, adaptive adjustment laws are addressed to adjust unknown coupling factors to restrain the unexpected action of the nonlinearly coupled networks. On the basis of Lyapunov stability theory, adaptive pinning controllers and coupling strength adjusters are constructed to ensure that the synchronization errors of the networks can be reduced as small as desired in the presence of the nonlinear couplings. A numerical simulation is provided to illustrate the effectiveness of the theoretical results.}}
@article{Song20123831,
