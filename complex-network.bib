issn = {1084-8045},
doi = {http://dx.doi.org/10.1016/j.jnca.2013.09.017},
url = {http://www.sciencedirect.com/science/article/pii/S1084804513002063},
author = {Francisco Macia-Perez and Iren Lorenzo-Fonseca and Jose Vicente Berna-Martinez},
keywords = {Network management systems},
