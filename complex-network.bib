volume = {274},
number = {},
pages = {273 - 285},
year = {2014},
note = {},
issn = {0020-0255},
doi = {http://dx.doi.org/10.1016/j.ins.2014.02.148},
url = {http://www.sciencedirect.com/science/article/pii/S0020025514002709},
author = {Xiao-Zheng Jin and Ju H. Park},
keywords = {Complex networked system},
keywords = {Insensitive tracking},
keywords = {Adaptive sliding mode control},
keywords = {Faulty communication signal},
keywords = {Controller coefficient variation},
abstract = {Abstract This paper addresses the problem of insensitive tracking control for a class of complex networked systems with faulty and perturbed networks and controller additive coefficient variations. The coupled networks are supposed to be faulted randomly and perturbed by bounded time delays and disturbances. Moreover, additive controller gain variations that can also be considered as actuator bias faults are proposed. Adaptive sliding mode compensation control schemes were developed to update control gains in order to eliminate these non-ideal factors. Then, a class of distributed robust adaptive sliding mode controllers was constructed to automatically compensate for the faulted and variational effects based on the information from adaptive schemes. On the basis of Lyapunov stability theory, it is shown that average tracking of the resulting adaptive complex networked systems can be achieved. An example is provided to further illustrate the effectiveness of the proposed adaptive sliding mode design technique.}}
@article{Li201478,
