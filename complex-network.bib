url = {http://www.sciencedirect.com/science/article/pii/S0893608015000301},
author = {Huaqing Li and Xiaofeng Liao and Guo Chen and David J. Hill and Zhaoyang Dong and Tingwen Huang},
keywords = {Event-triggering},
keywords = {Distributed control},
keywords = {Complex networks},
keywords = {Synchronization},
keywords = {Strongly connected network},
keywords = {Directed spanning tree},
abstract = {Abstract This paper presents a new framework for synchronization of complex network by introducing a mechanism of event-triggering distributed sampling information. A kind of event which avoids continuous communication between neighboring nodes is designed to drive the controller update of each node. The advantage of the event-triggering strategy is the significant decrease of the number of controller updates for synchronization task of complex networks involving embedded microprocessors with limited on-board resources. To describe the system's ability reaching synchronization, a concept about generalized algebraic connectivity is introduced for strongly connected networks and then extended to the strongly connected components of the directed network containing a directed spanning tree. Two sufficient conditions are presented to reveal the underlying relationships of corresponding parameters to reach global synchronization based on algebraic graph, matrix theory and Lyapunov control method. A positive lower bound for inter-event times is derived to guarantee the absence of Zeno behavior. Finally, a numerical simulation example is provided to demonstrate the theoretical results.}}
@article{Xiong201577,
title = {Forecasting interval time series using a fully complex-valued RBF neural network with DPSO and PSO algorithms},
journal = {Information Sciences},
