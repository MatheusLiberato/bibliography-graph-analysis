issn = {1875-5100},
doi = {http://dx.doi.org/10.1016/j.jngse.2016.03.032},
url = {http://www.sciencedirect.com/science/article/pii/S187551001630138X},
author = {Jianlei Sun and Amy Zou and Edith Sotelo and David Schechter},
keywords = {Unconventional Liquid Reservoirs (ULRs)},
keywords = {CO2 huff-n-puff},
keywords = {Production performance},
keywords = {Dual continuum model},
keywords = {discrete fracture network},
abstract = {Abstract Horizontal drilling and hydraulic fracturing produce Unconventional Liquid Reservoirs (ULRs) economically. One of the characteristics of the ULRs is high production declining rate, and thus enhanced oil recovery techniques such as Huff-n-Puff will ensue. In addition, the existence of complex fracture network has been proven by both field and simulation work. This study investigated the potential of CO2 huff-n-puff in ULRs with complex fracture networks. Two fracture characterization techniques for complex fracture network such as fractal-based, and microseismic-based were applied to reduce uncertainties of fracture networks and thus improve the accuracy of simulation input. In order to study complex CO2 recovery mechanism, lab-measured data was upscaled to the field-scale, and then used for evaluating production performance of a simple fracture network. Detailed sensitivity analysis showed that diffusion in the field scale is negligible compared with other recovery mechanisms such as oil swelling, gas expansion, viscosity reduction, etc. The following comparison between dual continuum and the explicit fracture modeling approaches highlights the importance of heterogeneity due to the explicit fractures. Dual continuum models yields less accurate pressure behavior than explicit discrete fracture models even though production behavior could be history matched.}}
@article{Wu20152110,
title = {Complex hybrid synchronization of complex-variable dynamical network via impulsive control},
journal = {Optik - International Journal for Light and Electron Optics},
volume = {126},
number = {19},
pages = {2110 - 2114},
year = {2015},
note = {},
issn = {0030-4026},
doi = {http://dx.doi.org/10.1016/j.ijleo.2015.05.084},
url = {http://www.sciencedirect.com/science/article/pii/S0030402615003976},
author = {Zhaoyan Wu},
keywords = {Complex hybrid synchronization},
keywords = {Dynamical network},
keywords = {Complex-variable},
keywords = {Impulsive control},
abstract = {Abstract In this paper, complex hybrid synchronization of network coupled with complex-variable chaotic systems is investigated. The complex-variable dynamical network is synchronized onto a given orbit with respect to a complex matrix using impulsive control. Adaptive strategy is adopted to design adaptive impulsive controllers, which is universal for different dynamical networks. Further, it can relax the restriction on impulsive intervals. Based on impulsive stability theory and Lyapunov function method, several synchronization criteria for achieving complex hybrid synchronization are derived. Numerical examples are provided to show the effectiveness of the theoretical results.}}
@article{Eustace2015665,
title = {Community detection using local neighborhood in complex networks},
journal = {Physica A: Statistical Mechanics and its Applications},
volume = {436},
number = {},
pages = {665 - 677},
year = {2015},
