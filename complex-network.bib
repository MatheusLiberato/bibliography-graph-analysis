url = {http://www.sciencedirect.com/science/article/pii/S0925231215016318},
author = {Xiaofeng Chen and Qiankun Song and Zhenjiang Zhao and Yurong Liu},
keywords = {Discrete-time complex-valued neural networks},
keywords = {μ-Stability},
keywords = {Leakage delay},
